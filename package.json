{
  "name": "restaurant-dashboard",
  "version": "1.0.0",
  "description": "Restaurant Management Dashboard with Real-time Analytics",
  "main": "server.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
